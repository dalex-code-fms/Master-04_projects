package fr.fms.poo;

public class City {
	public String city;
	public String country;
	public double nbPopulation;
	
	public City(String city, String country, double nbPopulation) {
		this.city = city;
		this.country = country;
		this.nbPopulation = nbPopulation;
	}
	
	public void display() {
		System.out.printf("city : %s%ncountry : %s%nPopulation : %1.3f%n%n", city, country, nbPopulation);
	}
}
